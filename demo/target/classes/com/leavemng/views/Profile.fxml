<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="437.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.leavemng.controllers.ProfileController">
    <Label text="Username:" style="-fx-font-size: 18.0; -fx-font-weight: bold;"/>
    <Label fx:id="usernameLabel" style="-fx-font-size: 18.0;"/>
    <Label text="Phone:" style="-fx-font-size: 18.0; -fx-font-weight: bold;"/>
    <Label fx:id="phoneLabel" style="-fx-font-size: 18.0;"/>
    <Label text="Department:" style="-fx-font-size: 18.0; -fx-font-weight: bold;"/>
    <Label fx:id="departementLabel" style="-fx-font-size: 18.0;"/>
    <Label text="Birth Date:" style="-fx-font-size: 18.0; -fx-font-weight: bold;"/>
    <Label fx:id="birthDateLabel" style="-fx-font-size: 18.0;"/>
    <Label text="Annual Balance:" style="-fx-font-size: 18.0; -fx-font-weight: bold;"/>
    <Label fx:id="annualBalanceLabel" style="-fx-font-size: 18.0;"/>
    <Button text="Go Back" onAction="#handleGoBackButtonAction" alignment="CENTER" mnemonicParsing="false" prefHeight="93.0" prefWidth="400.0" style="-fx-background-color: #298eb5;" textFill="WHITE">
        <font>
            <Font name="System Bold" size="18.0" />
        </font>
    </Button>
</VBox>